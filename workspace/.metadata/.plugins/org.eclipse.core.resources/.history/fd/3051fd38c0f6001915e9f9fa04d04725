import data.Database;

public class RecentDaoImpl implements RecentDao{
	private static RecentDaoImpl instance;
	
	private RecentDaoImpl(){
		
	}
	
	public static RecentDaoImpl getInstance(){
		if(instance == null)
		{
			instance = new RecentDaoImpl();
		}
		return instance;
	}
	
